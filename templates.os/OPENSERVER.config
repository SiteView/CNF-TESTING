id=OPENSERVER
name=OPENSERVER
#
id=disks
command=/bin/df -P
mount=6
name=1
#
id=disk
command=/bin/df -P <disk>
total=4
free=3
percentUsed=5
startLine=2
#
id=memory
command=/etc/swap -l
total=4
free=5
startLine=2
swapUnit=1024
#
id=pageFault
command=/usr/bin/vmstat -s
pageFaults=1
matchLine=pages paged
#
id=cpu
command=/usr/bin/vmstat
startline=5
idle=20
#wait=2
#
id=process
command=/usr/bin/ps -ef
nameColumnName=CMD
startLine=2
#
id=processDetail
command=ps -el
nameColumnName=CMD
sizeColumnName=SZ
startLine=2
#
id=filesize
command=/usr/bin/ls -l <file>
size=5
#
id=tail
command=/usr/bin/tail -c +<bytes> <file>
#
id=fileExists
command=/usr/bin/test -r <file>; echo $?
match=0
#
id=fileList
command=/usr/bin/ls -l <directory> | /usr/bin/grep -v "^total" | /usr/bin/grep -v "^d" | /usr/bin/sed 's/  */!/g' | /usr/bin/cut -d\! -f5,9
#
id=systemTime
command=echo ssDateStart; /usr/bin/date -u +"%m/%d/%Y %H:%M:%S"; /usr/bin/date +"%m/%d/%Y %H:%M:%S"
#
id=catFile
command=/usr/bin/cat <file>
