<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
       point="org.eclipse.ui.views">
    <view
          class="SiteView.ecc.views.EccTreeControl"
          id="SiteView.ecc.views.EccTreeControl"
          name="Group">
    </view>
    <view
          class="SiteView.ecc.views.EccReportView"
          id="SiteView.ecc.views.EccReportView"
          name="EccReportView"
          restorable="true">
    </view>
 </extension>
 <extension
       point="org.eclipse.ui.editors">
    <editor
          class="SiteView.ecc.editors.EccControl"
          id="SiteView.ecc.editors.EccControl"
          name="Detail">
    </editor>
 </extension>
 <extension
       point="Siteview.Forms.Common.LayoutExtension">
    <ExtViews_matchByStrategy
          Class="SiteView.ecc.tab.views.MonitorLogTabView"
          Title="日志数据"
          match_strategy="SiteView.ecc.tab.views.MatchClass">
    </ExtViews_matchByStrategy>
    <ExtViews_matchByStrategy
          Class="SiteView.ecc.tab.views.SummaryTabView"
          Title="概要"
          match_strategy="SiteView.ecc.tab.views.MatchClass">
    </ExtViews_matchByStrategy>
 </extension>
 </plugin>
